{
  "name": "@marketgoo/ola",
  "version": "0.4.0-beta.1",
  "description": "Design System by marketgoo",
  "main": "dist/index.js",
  "styles": "dist/index.css",
  "license": "MIT",
  "scripts": {
    "start": "concurrently npm:watch:css npm:guide",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "npm run lint && jest ./src",
    "test:update": "jest ./src -u",
    "watch": "concurrently npm:watch:js npm:watch:css",
    "watch:js": "babel ./src -d dist --ignore 'src/**/stories.js','src/**/test.js' --watch",
    "watch:css": "postcss src/index.css -o dist/index.css --watch",
    "guide": "start-storybook -p 6006",
    "build": "rm -rf dist && mkdir dist && babel ./src -d dist --ignore 'src/**/stories.js','src/**/test.js' && postcss src/index.css -o dist/index.css",
    "build:storybook": "build-storybook",
    "publish:storybook": "npm run build:storybook && gh-pages -d storybook-static",
    "prepack": "npm test && npm run build"
  },
  "homepage": "https://github.com/marketgoo/Ola.git",
  "keywords": [
    "react",
    "design-system",
    "ola",
    "component-library"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/marketgoo/Ola.git"
  },
  "files": [
    "dist",
    "src",
    "*.md",
    "ola.png",
    "LICENSE"
  ],
  "peerDependencies": {
    "react": "^16.8.6",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@storybook/addon-actions": "^5.3.6",
    "@storybook/addon-info": "^5.3.6",
    "@storybook/addon-knobs": "^5.3.6",
    "@storybook/addon-links": "^5.3.6",
    "@storybook/addon-viewport": "^5.3.6",
    "@storybook/addons": "^5.3.6",
    "@storybook/react": "^5.3.6",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.2",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.18.0",
    "gh-pages": "^2.2.0",
    "jest": "^24.9.0",
    "postcss": "^7.0.26",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-prefix-selector": "^1.7.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^8.0.0",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-test-renderer": "16.8.6"
  }
}
